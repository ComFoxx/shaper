  "main": "build/server/index.js",
  "type": "module",
  "engines": {
    "node": "17.x"
  },
  "scripts": {
    "dev": "concurrently \"npm:start:watch\" \"vite\"",
    "start:watch": "nodemon --ignore state.json src/server/index.ts",
    "start": "npx esmo ./src/server/index.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "coverage": "vitest --coverage",
    "build": "tsc && vite build",
    "lint": "tsc",
    "preview": "vite preview",
    "visualize": "esmo visualizer.ts"
  },
  "nodemonConfig": {
    "exec": "esmo",
    "ext": "js,json,ts,tsx"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
