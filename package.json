    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.2",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^7.0.0",
    "esmo": "^0.14.0",
    "heroku": "^7.59.2",
    "localtunnel": "^2.0.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.2",
    "vitest": "^0.1.27"
  },
  "dependencies": {
    "canvas-confetti": "^1.4.0",
